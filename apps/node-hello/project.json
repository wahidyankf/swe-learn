{
  "name": "node-hello",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/node-hello/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run node-hello:build"
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        }
      ]
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run node-hello:dev"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run node-hello:start"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/node-hello/**/*.ts"]
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p apps/node-hello/tsconfig.json --noEmit"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["node-hello-libs"]
}
