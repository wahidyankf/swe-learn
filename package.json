{
  "name": "@swe-learn/source",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepare": "husky install && chmod ug+x .husky/*",
    "affected:test": "nx affected --target=test",
    "affected:test:parallel": "nx affected --target=test --parallel",
    "affected:build": "nx affected --target=build",
    "affected:lint": "nx affected --target=lint",
    "affected:typecheck": "nx affected --target=typecheck",
    "affected:graph": "nx affected:graph",
    "node:app:build": "nx run node-hello:build",
    "node:app:dev": "nx run node-hello:serve",
    "node:app:test": "nx run node-hello:test",
    "node:app:lint": "nx run node-hello:lint",
    "node:lib:build": "nx run node-hello-libs:build",
    "node:lib:test": "nx run node-hello-libs:test",
    "node:lib:lint": "nx run node-hello-libs:lint",
    "node:format": "nx run-many --target=format --projects=node-hello,node-hello-libs",
    "node:test": "nx run-many --target=test --projects=node-hello,node-hello-libs",
    "node:lint": "nx run-many --target=lint --projects=node-hello,node-hello-libs",
    "python:app:install": "nx run python-hello:install",
    "python:app:dev": "nx run python-hello:serve",
    "python:app:test": "nx run python-hello:test",
    "python:app:lint": "nx run python-hello:lint",
    "python:lib:install": "nx run python-hello-libs:install",
    "python:lib:build": "nx run python-hello-libs:build",
    "python:lib:test": "nx run python-hello-libs:test",
    "python:lib:lint": "nx run python-hello-libs:lint",
    "python:format": "nx run-many --target=format --projects=python-hello,python-hello-libs",
    "python:test": "nx run-many --target=test --projects=python-hello,python-hello-libs",
    "python:lint": "nx run-many --target=lint --projects=python-hello,python-hello-libs",
    "go:app:install": "nx run go-hello:install",
    "go:app:dev": "nx run go-hello:serve",
    "go:app:build": "nx run go-hello:build",
    "go:app:test": "nx run go-hello:test",
    "go:app:lint": "nx run go-hello:lint",
    "go:lib:install": "nx run go-hello-libs:install",
    "go:lib:build": "nx run go-hello-libs:build",
    "go:lib:test": "nx run go-hello-libs:test",
    "go:lib:lint": "nx run go-hello-libs:lint",
    "go:format": "nx run-many --target=format --projects=go-hello,go-hello-libs",
    "go:test": "nx run-many --target=test --projects=go-hello,go-hello-libs",
    "go:lint": "nx run-many --target=lint --projects=go-hello,go-hello-libs",
    "java:app:build": "nx run java-hello:build",
    "java:app:dev": "nx run java-hello:serve",
    "java:app:test": "nx run java-hello:test",
    "java:app:lint": "nx run java-hello:lint",
    "java:lib:build": "nx run java-hello-libs:build",
    "java:lib:test": "nx run java-hello-libs:test",
    "java:lib:lint": "nx run java-hello-libs:lint",
    "java:format": "./scripts/format-java.sh",
    "java:test": "nx run-many --target=test --projects=java-hello,java-hello-libs",
    "java:lint": "nx run-many --target=lint --projects=java-hello,java-hello-libs",
    "all:build": "nx run-many --target=build --all",
    "all:test": "nx run-many --target=test --all",
    "all:lint": "nx run-many --target=lint --all",
    "all:typecheck": "nx run-many --target=typecheck --all",
    "all:format": "nx run-many --target=format --all",
    "all:check": "npm run all:build && npm run all:test && npm run all:lint && npm run all:typecheck"
  },
  "private": true,
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@nx/eslint-plugin": "^20.3.3",
    "@nx/js": "20.3.3",
    "@nx/node": "^20.3.3",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitest/coverage-v8": "^3.0.4",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "nodemon": "^3.1.9",
    "nx": "20.3.3",
    "prettier": "^2.8.8",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.3.0",
    "tsx": "^4.19.2",
    "typescript": "~5.6.2",
    "vitest": "^3.0.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css,json,yaml,yml}": [
      "prettier --write"
    ],
    "apps/go-hello/**/*.go": [
      "cd apps/go-hello && go fmt"
    ],
    "libs/go-hello-libs/**/*.go": [
      "cd libs/go-hello-libs && go fmt"
    ]
  },
  "workspaces": [
    "packages/*"
  ],
  "volta": {
    "node": "20.18.2",
    "npm": "11.0.0"
  }
}
