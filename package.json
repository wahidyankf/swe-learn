{
  "name": "@swe-learn/source",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "affected:build": "nx affected --target=build",
    "affected:graph": "nx affected:graph",
    "affected:lint": "nx affected --target=lint",
    "affected:test:parallel": "nx affected --target=test --parallel",
    "affected:test": "nx affected --target=test",
    "affected:typecheck": "nx affected --target=typecheck",
    "all:build": "nx run-many --target=build --all",
    "all:check": "npm run all:build && npm run all:test && npm run all:lint && npm run all:typecheck",
    "all:format": "nx run-many --target=format --all",
    "all:lint": "nx run-many --target=lint --all",
    "all:test": "nx run-many --target=test --all",
    "all:typecheck": "nx run-many --target=typecheck --all",
    "apps:go-hello:build": "nx run go-hello:build",
    "apps:go-hello:dev": "nx run go-hello:serve",
    "apps:go-hello:install": "nx run go-hello:install",
    "apps:go-hello:lint": "nx run go-hello:lint",
    "apps:go-hello:test": "nx run go-hello:test",
    "apps:java-hello:build": "nx run java-hello:build",
    "apps:java-hello:dev": "nx run java-hello:serve",
    "apps:java-hello:lint": "nx run java-hello:lint",
    "apps:java-hello:test": "nx run java-hello:test",
    "apps:node-hello:build": "nx run node-hello:build",
    "apps:node-hello:dev": "nx run node-hello:serve",
    "apps:node-hello:lint": "nx run node-hello:lint",
    "apps:node-hello:test": "nx run node-hello:test",
    "apps:python-hello:dev": "nx run python-hello:serve",
    "apps:python-hello:install": "nx run python-hello:install",
    "apps:python-hello:lint": "nx run python-hello:lint",
    "apps:python-hello:test": "nx run python-hello:test",
    "libs:go-hello-libs:build": "nx run go-hello-libs:build",
    "libs:go-hello-libs:install": "nx run go-hello-libs:install",
    "libs:go-hello-libs:lint": "nx run go-hello-libs:lint",
    "libs:go-hello-libs:test": "nx run go-hello-libs:test",
    "libs:java-hello-libs:build": "nx run java-hello-libs:build",
    "libs:java-hello-libs:lint": "nx run java-hello-libs:lint",
    "libs:java-hello-libs:test": "nx run java-hello-libs:test",
    "libs:node-hello-libs:build": "nx run node-hello-libs:build",
    "libs:node-hello-libs:lint": "nx run node-hello-libs:lint",
    "libs:node-hello-libs:test": "nx run node-hello-libs:test",
    "libs:python-hello-libs:build": "nx run python-hello-libs:build",
    "libs:python-hello-libs:install": "nx run python-hello-libs:install",
    "libs:python-hello-libs:lint": "nx run python-hello-libs:lint",
    "libs:python-hello-libs:test": "nx run python-hello-libs:test",
    "prepare": "husky install && chmod ug+x .husky/*"
  },
  "private": true,
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@nx/eslint-plugin": "^20.3.3",
    "@nx/js": "20.3.3",
    "@nx/node": "^20.3.3",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitest/coverage-v8": "^3.0.4",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "nodemon": "^3.1.9",
    "nx": "20.3.3",
    "prettier": "^2.8.8",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.3.0",
    "tsx": "^4.19.2",
    "typescript": "~5.6.2",
    "vitest": "^3.0.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css,json,yaml,yml}": [
      "prettier --write"
    ],
    "apps/go-hello/**/*.go": [
      "cd apps/go-hello && go fmt"
    ],
    "libs/go-hello-libs/**/*.go": [
      "cd libs/go-hello-libs && go fmt"
    ]
  },
  "workspaces": [
    "packages/*"
  ],
  "volta": {
    "node": "20.18.2",
    "npm": "11.0.0"
  }
}
