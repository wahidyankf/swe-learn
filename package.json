{
  "name": "@swe-learn/source",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepare": "husky install && chmod ug+x .husky/*",
    "test:affected": "nx affected --target=test",
    "test:affected:parallel": "nx affected --target=test --parallel",
    "build:affected": "nx affected --target=build",
    "lint:affected": "nx affected --target=lint",
    "typecheck:affected": "nx affected --target=typecheck",
    "affected:graph": "nx affected:graph",
    "node-hello:build": "tsc -p apps/node-hello/tsconfig.json",
    "node-hello:dev": "tsx watch apps/node-hello/src/main.ts",
    "node-hello:start": "node dist/apps/node-hello/main.js",
    "python-hello:install": "nx run python-hello:install",
    "python-hello:dev": "nx run python-hello:serve",
    "python-hello-libs:install": "nx run python-hello-libs:install",
    "python-hello-libs:build": "nx run python-hello-libs:build",
    "format:python": "nx run-many --target=format --projects=python-hello,python-hello-libs"
  },
  "private": true,
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@nx/eslint-plugin": "^20.3.3",
    "@nx/js": "20.3.3",
    "@nx/node": "^20.3.3",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitest/coverage-v8": "^3.0.4",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "nodemon": "^3.1.9",
    "nx": "20.3.3",
    "prettier": "^2.8.8",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.3.0",
    "tsx": "^4.19.2",
    "typescript": "~5.6.2",
    "vitest": "^3.0.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "workspaces": [
    "packages/*"
  ],
  "volta": {
    "node": "20.18.2",
    "npm": "11.0.0"
  }
}
